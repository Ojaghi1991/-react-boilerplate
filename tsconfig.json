{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "paths": {
      "assets": ["assets"],
      "components": ["src/components"],
      "components/*": ["src/components/*"],
      "utils": ["src/utils"],
      "config": ["config"],
      "containers/*": ["src/containers/*"],
      "helpers": ["src/helpers"],
      "helpers/*": ["src/helpers/*"],
      "redux/*": ["src/redux/*"],
      "themes": ["src/themes"]
    },
    "target": "es6",
    "types": ["node"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["./node_modules/**/*"]
}
